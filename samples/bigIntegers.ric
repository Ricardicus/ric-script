print("Ett stort tal: " + pow(bigInt(81), 1000))

# In conditions
? [ 9999999999999999999 ] {
  print("hej")
}

? [ 9999999999999999999 > 0 ] {
  print("hejsan")
}

? [ 9999999999999999999 < 10 ] {
  print("inget hej")
}

? [ 10 < 9999999999999999999 ] {
  print("hej igen")
}

? [ 9999999999999999999 <= 9999999999999999999 ] {
  print("hejsan igen")
}

