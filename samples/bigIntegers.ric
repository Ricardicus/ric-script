print(s)

s = s * 1000000
print(s)

s = 2 + 32132132132132132
print(s)

s = 231321321321321 + 2
print(s)

s = 9999999999999 - 8888888888888
print(s)

s = s - 9999999999999 
print(s)

s = 88888888888888 % 77777777777777
print(s)

s = 6666666666666666 / 3
print(s)

