(ls (".") ... file ) {
  # on Windows, .\\ is prefixed to the file lists,
  # I make sure it works on both
  ? [ file == "requirements.txt" ||
      file == ".\\requirements.txt" ] {
    fp = fileOpen(file)
    d = fileRead(fp, 100)
    print(list(d))
    fileClose(fp)
  }
}